# Base URL for the Fiindo API
FIINDO_API_BASE=https://api.test.fiindo.com

# Authentication token/username for Fiindo API
FIINDO_AUTH=your_fiindo_auth_token_here


# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO


# Maximum number of parallel workers for concurrent processing in etl service
ETL_MAX_WORKERS=10


# HTTP request timeout in seconds
HTTP_TIMEOUT=10

# Number of retries for failed HTTP requests
HTTP_RETRIES=3


# database URL
DATABASE_URL=your_database_url_here
